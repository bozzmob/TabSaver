{
    "name": "saveTabs",
    "version": "1.1.1",
    "description": "save your tabs",
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_title": "save_tabs",
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://cdnjs.cloudflare.com; object-src 'self'"
}